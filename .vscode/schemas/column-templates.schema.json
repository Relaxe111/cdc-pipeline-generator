{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Column Templates",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "templates": {
      "type": "object",
      "minProperties": 1,
      "additionalProperties": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "name": { "type": "string" },
          "type": { "type": "string" },
          "not_null": { "type": "boolean" },
          "description": { "type": "string" },
          "value": { "type": "string" },
          "default": { "type": "string" }
        },
        "required": ["name", "type"]
      }
    }
  },
  "required": ["templates"]
}