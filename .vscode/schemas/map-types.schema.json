{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Type Mapping",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "metadata": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "version": { "type": ["integer", "string"] }
      }
    },
    "aliases": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": { "type": "string" }
      }
    },
    "mappings": {
      "type": "object",
      "additionalProperties": { "type": "string" }
    },
    "compatibility": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": { "type": "string" }
      }
    }
  },
  "required": ["mappings"]
}